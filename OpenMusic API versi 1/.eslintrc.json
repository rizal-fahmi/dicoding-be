{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        // "no-console": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "object-curly-newline": "off",
        "max-len": "off",
        "no-else-return": "off",
        "comma-dangle": "off",
    }
}